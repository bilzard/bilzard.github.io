{
    "[jekyll]": {
        "editor.quickSuggestions": {
            "comments": "on",
            "strings": "on",
            "other": "on"
        },
        "editor.formatOnSave": true,
        "editor.wordWrap": "on",
        "editor.renderWhitespace": "all",
        "editor.acceptSuggestionOnEnter": "off"
    },
    "files.associations": {
        "*.html": "liquid",
        "*.liquid": "liquid",
        "*.css": "jekyll",
        "*.scss": "jekyll",
    },
}